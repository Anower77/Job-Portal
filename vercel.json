{
  "builds": [{
    "src": "job/wsgi.py",
    "use": "@vercel/python"
  }, {
    "src": "build_files.sh",
    "use": "@vercel/static-build",
    "config": {
      "distDir": "staticfiles"
    }
  }],
  "routes": [{
    "src": "/static/(.*)",
    "dest": "/static/$1"
  }, {
    "src": "/(.*)",
    "dest": "job/wsgi.py"
  }],
  "ignoreCommand": "git diff --quiet HEAD^ HEAD ./",
  "git": {
    "deploymentEnabled": {
      "pip": false,
      "env": false,
      "debug.log": false,
      "__pycache__": true
    }
  }
}


